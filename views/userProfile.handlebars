{{!--
<div class="container"> --}}
  <div class="section">
    <div class="row">
      <div class="col s4 m4">
        <div class="icon-block">

          <div class="row">
            <div class="col s12">
              <div class="card horizontal">
                <div class="card-image">
                  <img src="{{ user.user_pic_profile }}" alt="" class="responsive-img">
                </div>
                <div class="card-stacked">
                  <div class="card-content">
                    <p>
                      {{ user.user_First_name }} {{ user.user_Last_name }}
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- <h2 class="center light-blue-text"><i class="material-icons">flash_on</i></h2> -->
          <!-- <h5 class="center">Speeds up development</h5> -->
          <div class="row">
            <div class="col s12">
              <div class="card-panel white">
                <span class="black-text">About Me
                  <div id="story">
                    <p class="userBio">
                      {{ user.user_bio_info }}
                    </p>

                  </div>
                </span>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col s12">

              <div class="card-panel white">
                <span class="black-text">Pictures
                </span>
              </div>

            </div>
          </div>

          <!-- icon-block -->
        </div>
      </div>

      <div class="col s8 m8">
        <div class="icon-block">
          <!-- <div class="icon-block">
              <h2 class="center light-blue-text"><i class="material-icons">group</i></h2> -->
          <!-- <h5 class="center">User Experience Focused</h5> -->

          <div class="row">
            <div class="col s12">
              <div class="card">
                <div class="card-image">
                  <h1>Click countries to select</h1>
                  <div id="chartdiv"></div>
                  <div id="info">Seletced countries:
                    <span id="selected">-</span>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <section>
            <div class="col s12">
              <h4 class="header white-text">My Stories</h4>
              <div class="row">
                <div class="card horizontal">

                  {{!--
                  <div class="col s4"> --}}
                    <div class="card-image">
                      <img src="https://cdn.pixabay.com/photo/2018/05/13/17/18/paris-3397173_1280.jpg" alt="" class="card-image responsive-img storyImg">
                    </div>

                      <div class="card-stacked">
                        <h4 class="card-title">Story Title
                          <i class="material-icons right">more_vert</i>
                        </h4>
                        {{#each blog}} {{ dataValues.user_StoryList }} {{/each}}
                      </div>
                      <div class="card-action">
                        <a href="/api/blog/{{ user.client_id }}" target="_blank">Read Story</a>
                      </div>
                    </div>
                </div>
              </div>
            </div>
          </section>

          <div class="row">
            <div class="col s12">
              <div class="card-panel white">
                <h4 class="title">Recommendations</h4>


                <ul class="collection with-header">
                  <li class="collection-header">
                    <h5>Restaurants/Dining</h5>
                  </li>
                  {{#each food}}
                  <li class="collection-item avatar">
                    <i class="material-icons circle green">local_dining</i>
                    <P>{{ dataValues.user_rec_food }}</P>
                  </li>
                  {{/each}}
                </ul>

                <ul class="collection with-header">
                  <li class="collection-header">
                    <h5>Attractions</h5>
                  </li>
                  {{#each attractions }}
                  <li class="collection-item avatar">
                    <i class="material-icons circle blue">photo_camera</i>
                    <P>{{ dataValues.user_rec_att }}</P>
                  </li>
                  {{/each}}
                </ul>

                <ul class="collection with-header">
                  <li class="collection-header">
                    <h5>Events</h5>
                  </li>
                  {{#each events }}
                  <li class="collection-item avatar">
                    <i class="material-icons circle red">photo_camera</i>
                    <P>{{ dataValues.user_rec_eve }}</P>
                  </li>
                  {{/each}}
                </ul>


              </div>
            </div>
          </div>


          <!-- icon block -->
        </div>
        <!-- col s9 -- >
  </div>
              <!-- row -->
      </div>

      <!-- section -->
    </div>
   
    </div>

    <script src="/js/userProfileInfo.js"></script>