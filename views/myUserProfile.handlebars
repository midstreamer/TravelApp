  <div class="section">
    <div class="row">
      <div class="col s4 m4">
        <div class="icon-block">

          <div class="row">
            <div class="col s12">
              <div class="card horizontal">
                <div class="card-image">
                  <img src="{{ user.user_pic_profile }}" alt="" class="responsive-img">
                </div>
                <div class="card-stacked">
                  <div class="card-content">
                    <p>
                      {{ user.user_First_name }} {{ user.user_Last_name }}
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- <h2 class="center light-blue-text"><i class="material-icons">flash_on</i></h2> -->
          <!-- <h5 class="center">Speeds up development</h5> -->
          <div class="row">
            <div class="col s12">
              <div class="card-panel white">
                <span class="black-text">About Me
                  <div id="story">
                    <p class="userBio">
                      {{ user.user_bio_info }}
                    </p>

                  </div>
                </span>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col s12">

              <div class="card-panel white">
                <span class="black-text">Pictures
                </span>
              </div>

            </div>
          </div>

          <!-- icon-block -->
        </div>
      </div>

      <div class="col s8 m8">
        <div class="icon-block">
          <!-- <div class="icon-block">
              <h2 class="center light-blue-text"><i class="material-icons">group</i></h2> -->
