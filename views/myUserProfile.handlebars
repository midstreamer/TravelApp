          <div class="row">
            <div class="col s12">
              <div class="card-panel white">
                <h4 class="title">Recommendations</h4>

                {{!-- food --}}
                <ul class="collection with-header">
                  <li class="collection-header">
                    <h5>Restaurants/Dining
                      <i id="foodClick" class="material-icons">add_circle_outline</i>
                    </h5>
                    <div id='foodToHide' class="row">
                      <i id='foodCloseToHide' class="material-icons">close</i>

                      <div class="input-field col s4">
                        <textarea id="food_input" class="materialize-textarea"></textarea>
                        <label class="active" for="Food">Food</label>
                        <a id="foodOnSubmit" class="waves-effect waves-light btn-small">Add</a>
                      </div>
                    </div>
                  </li>
                  {{#each food}}
                  <li class="collection-item avatar">
                    <i class="material-icons circle green">local_dining</i>
                    <P>{{ dataValues.user_rec_food }}</P>

                    <form id="updateFood" action="/api/food/remove/{{this.id}}" method="POST">
                      <input type="hidden" name="toDelete">
                      <button class="btn btn-danger" type="submit">delete</button>
                    </form>

                  </li>
                  {{/each}}
                </ul>
                {{!--Attractions --}}
                <ul class="collection with-header">
                  <li class="collection-header">
                    <h5>Attractions
                      <i id="attractionsClick" class="material-icons">add_circle_outline</i>
                    </h5>

                    {{!-- --}}


                    <div id='attractionsToHide' class="row">
                      <i id='AttractionsCloseToHide' class="material-icons">close</i>
                      <div class="input-field col s4">
                        <textarea id="attractions_input" class="materialize-textarea"></textarea>
                        <label class="active" for="attractions">attractions</label>
                        <a id="attractionsOnSubmit" class="waves-effect waves-light btn-small">Add</a>
                      </div>
                    </div>
                    {{!-- --}}
                  </li>
                  {{#each attractions }}
                  <li class="collection-item avatar">
                    <i class="material-icons circle blue">photo_camera</i>
                    <P>{{ dataValues.user_rec_att }}</P>

                    <form id="updateAtt" action="/api/att/remove/{{this.id}}" method="POST">
                      <input type="hidden" name="toDelete">
                      <button class="btn btn-danger" type="submit">delete</button>
                    </form>

                  </li>
                  {{/each}}
                </ul>
                {{!-- Events --}}
                <ul class="collection with-header">
                  <li class="collection-header">
                    <h5>Events
                      <i id="eventsClick" class="material-icons">add_circle_outline</i>
                    </h5>
                    {{!-- --}}
                    <div id='eventsToHide' class="row">
                      <i id='eventsCloseToHide' class="material-icons">close</i>
                      <div class="input-field col s4">
                        <textarea id="events_input" class="materialize-textarea"></textarea>
                        <label class="active" for="events">events</label>
                        <a id="eventsOnSubmit" class="waves-effect waves-light btn-small">Add</a>
                      </div>
                    </div>
                    {{!-- --}}
                  </li>
                  {{#each events }}
                  <li class="collection-item avatar">
                    <i class="material-icons circle blue">photo_camera</i>
                    <P>{{dataValues.user_rec_eve}}</P>

                    <form id="update" action="/api/event/remove/{{this.id}}" method="POST">
                      <input type="hidden" name="toDelete">
                      <button class="btn btn-danger" type="submit">delete</button>
                    </form>

                  </li>
                  {{/each}}
                </ul>


                <!-- icon block -->
              </div>
              <!-- col s9 -- >
  </div>
              <!-- row -->
            </div>

            <!-- section -->
          </div>
          <!-- container -->
          {{!-- </div> --}}

        <script src="/js/recommendations.js"></script>